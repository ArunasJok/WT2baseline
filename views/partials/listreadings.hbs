<table class="ui table">
  <thead>
    <tr>
      <th>Name</th>
      <th>Weather</th>
      <th>Temperature</th>
      <th>Wind</th>
      <th>Pressure</th>
    </tr>
  </thead>
  <tbody>
    {{#each station.readings}}
      <tr>
        <td>
          {{name}}
        </td>
        <td>
          {{r}}
        </td>
      </tr>
    {{/each}}
  </tbody>
</table>